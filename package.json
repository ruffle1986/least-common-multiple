{
  "name": "least-common-multiple",
  "version": "1.0.0",
  "description": "Computes the least common multiple of the given integers.",
  "license": "MIT",
  "repository": "ruffle1986/least-common-multiple",
  "type": "module",
  "author": {
    "name": "ruffle1986"
  },
  "main": "./lib/index.js",
  "scripts": {
    "test": "xo",
    "posttest": "ava",
    "build": "babel lib -d dist",
    "prepublish": "npm test && npm run build",
    "cover": "istanbul cover ./node_modules/.bin/ava test.js",
    "coveralls": "npm run cover && cat ./coverage/lcov.info | ./node_modules/.bin/coveralls"
  },
  "xo": {
    "ignores": [
      "dist/**"
    ],
    "envs": [
      "node"
    ],
    "space": true,
    "esnext": true,
    "semicolon": false
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "least",
    "lowest",
    "smallest",
    "common",
    "multiple",
    "lcm",
    "mathematics",
    "es6",
    "esnext",
    "es2015"
  ],
  "devDependencies": {
    "ava": "git://github.com/sindresorhus/ava",
    "babel": "^5.8.23",
    "coveralls": "^2.11.4",
    "istanbul": "^0.3.19",
    "xo": "*"
  },
  "dependencies": {
    "greatest-common-divisor": "^1.0.0"
  }
}
